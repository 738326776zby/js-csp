{
  "name": "js-csp",
  "version": "0.6.0",
  "description": "CSP channels with ES6 generators, inspired by Clojurescript's core.async and Go",
  "keywords": [
    "csp",
    "async",
    "concurrency",
    "channel",
    "go",
    "clojure",
    "clojurescript",
    "core.async",
    "es6",
    "generators",
    "yield",
    "coroutines"
  ],
  "author": "Nguyễn Tuấn Anh <ubolonton@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubolonton/js-csp.git"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/ubolonton/js-csp/raw/master/LICENSE"
  },
  "bugs": {
    "email": "ubolonton@gmail.com",
    "url": "https://github.com/ubolonton/js-csp/issues"
  },
  "devDependencies": {
    "babel-plugin-rewire": "^1.0.0-rc-5",
    "chai": "^3.5.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "ncp": "^2.0.0",
    "rimraf": "^2.5.4",
    "transducers.js": "^0.3.2",
    "webpack": "^2.1.0-beta.20"
  },
  "scripts": {
    "clean": "rimraf lib build",
    "test": "./scripts/test",
    "test:watch": "./scripts/test-watch",
    "test:coverage": "./scripts/test-coverage",
    "build:lib": "node -e \"require('ncp').ncp('./src', './lib')\" && BABEL_ENV=production babel -d lib src",
    "build:browser": "webpack --config webpack.config.js",
    "build": "npm run build:lib && npm run build:node",
    "prebulish": "npm run build"
  },
  "engines": {
    "node": ">= 4.1.0",
    "npm": ">=3.0.0"
  },
  "main": "./lib/csp.js",
  "module": "./src/csp.js",
  "files": [
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
